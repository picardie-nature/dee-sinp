<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sinp="http://inpn.mnhn.fr/sinp/" xmlns:gml="http://www.opengis.net/gml/3.2" targetNamespace="http://inpn.mnhn.fr/sinp/" elementFormDefault="qualified" version="0.9.3">
  <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
  <xs:annotation>
    <xs:documentation source="nom">Occurrence de Taxon</xs:documentation>
    <xs:documentation source="versionDictionnaire">1.0</xs:documentation>
    <xs:documentation source="versionSchemaXSD">1.beta</xs:documentation>
    <xs:documentation source="auteurs">SINP</xs:documentation>
    <xs:documentation source="statutDoc">En cours validation</xs:documentation>
    <xs:documentation source="description">Ce schéma permet de d’échanger les données de biodiversité « occurrence de taxon » entre les plateformes régionales/thématiques et la plateforme nationale du Système d’Information Nature et Paysages</xs:documentation>
  </xs:annotation>
  <xs:element name="AttributAdditionel" type="sinp:AttributAdditionelType" substitutionGroup="gml:AbstractObject"/>
  <xs:complexType name="AttributAdditionelType">
    <xs:sequence>
      <xs:element name="nomAttribut" type="xs:string">
        <xs:annotation>
          <xs:documentation>Indique le libellé court de l’attribut ajouté en attribut additionnel</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="definitionAttribut" type="xs:string">
        <xs:annotation>
          <xs:documentation>Définition de l’attribut ajouté suffisamment précise pour permettre de comprendre le sujet de l’attribut ajouté.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="valeurAttribut" type="xs:string">
        <xs:annotation>
          <xs:documentation>Valeur qualitative ou quantitative de l’attribut additionnel</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="thematiqueAttribut" type="xs:string">
        <xs:annotation>
          <xs:documentation>Indique la thématique par un mot clé pour lequel l’attribut a été ajouté</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="uniteAttribut" type="xs:string">
        <xs:annotation>
          <xs:documentation>Unité de mesure de l’attribut. Exemple : Degré Celsius, mètre, Kilogramme. Si l’information ajoutée est de type quantitatif alors cet attribut doit être renseigné</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AttributAdditionelPropertyType">
    <xs:sequence>
      <xs:element ref="sinp:AttributAdditionel"/>
    </xs:sequence>
    <xs:attributeGroup ref="gml:OwnershipAttributeGroup"/>
  </xs:complexType>
  <xs:simpleType name="CodeRefHabitatValueType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="PAL">
        <xs:annotation>
          <xs:documentation>Classification paléarctique</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BBMEDFR">
        <xs:annotation>
          <xs:documentation>biocénoses benthiques de Méditerranée</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BRYOSOCIO">
        <xs:annotation>
          <xs:documentation>Synopsis bryosociologique</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PVF">
        <xs:annotation>
          <xs:documentation>Prodrome des végétations de France</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="PALSPM">
        <xs:annotation>
          <xs:documentation>Habitats de St Pierre et Miquelon</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ANTMER">
        <xs:annotation>
          <xs:documentation>Habitats marins des départements d'outre-mer des Antilles</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GUYMER">
        <xs:annotation>
          <xs:documentation>Habitats marins du département d'outre-mer de Guyane</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CORINEBIOTOPES">
        <xs:annotation>
          <xs:documentation>CORINE Biotopes</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="EUNIS">
        <xs:annotation>
          <xs:documentation>EUNIS Habitas</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="GMRC">
        <xs:annotation>
          <xs:documentation>Géomorphologie des récifs coralliens</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CH">
        <xs:annotation>
          <xs:documentation>Cahier d'habitat</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OSPAR">
        <xs:annotation>
          <xs:documentation>Convention OSPAR</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="BARC">
        <xs:annotation>
          <xs:documentation>Convention de Barcelone</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REBENT">
        <xs:annotation>
          <xs:documentation>Habitat benthique côtier (Bretagne)</xs:documentation>
        </xs:annotation>
